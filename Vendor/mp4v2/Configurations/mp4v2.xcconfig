PRODUCT_NAME=mp4v2
CURRENT_PROJECT_VERSION=2.1.0
DYLIB_CURRENT_VERSION=2.1.0
FRAMEWORK_VERSION=2.1.0
DYLIB_COMPATIBILITY_VERSION=2

GCC_NO_COMMON_BLOCKS=YES
CLANG_CXX_LANGUAGE_STANDARD=gnu++98

GCC_PREPROCESSOR_DEFINITIONS=$(inherited) HAVE_CONFIG_H
INFOPLIST_FILE[sdk=macosx*]=mp4v2-Info.plist
HEADER_SEARCH_PATHS=$(inherited) $(SRCROOT)/include
USER_HEADER_SEARCH_PATHS=$(inherited) $(SRCROOT) $(SRCROOT)/mp4v2-src $(SRCROOT)/mp4v2-src/include

GCC_WARN_64_TO_32_BIT_CONVERSION=NO
GCC_WARN_ABOUT_MISSING_PROTOTYPES=NO
GCC_WARN_SHADOW=NO
GCC_WARN_UNUSED_VARIABLE=NO
CLANG_WARN_IMPLICIT_SIGN_CONVERSION=NO
CLANG_WARN_SUSPICIOUS_IMPLICIT_CONVERSION=NO
WARNING_CFLAGS=-Wno-invalid-source-encoding
