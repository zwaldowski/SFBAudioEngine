PRODUCT_NAME=FLAC
CURRENT_PROJECT_VERSION=1.3.0
DYLIB_CURRENT_VERSION=1.3.0
FRAMEWORK_VERSION=1.3.0
DYLIB_COMPATIBILITY_VERSION=1

GCC_WARN_64_TO_32_BIT_CONVERSION=NO
GCC_WARN_UNUSED_VARIABLE=NO
CLANG_WARN_IMPLICIT_SIGN_CONVERSION=NO
CLANG_WARN_SUSPICIOUS_IMPLICIT_CONVERSION=NO
CLANG_WARN_ENUM_CONVERSION=NO
WARNING_CFLAGS=-Wno-tautological-constant-out-of-range-compare

GCC_PREPROCESSOR_DEFINITIONS=$(inherited) HAVE_CONFIG_H
INFOPLIST_FILE[sdk=macosx*]=$(SRCROOT)/FLAC/FLAC-Info.plist
HEADER_SEARCH_PATHS=$(inherited) $(SRCROOT)/FLAC/flac-src/include $(SRCROOT)/FLAC
USER_HEADER_SEARCH_PATHS=$(inherited) $(SRCROOT)/FLAC/flac-src/include $(SRCROOT)/FLAC/flac-src/src/libFLAC/include $(SRCROOT)/FLAC
ALWAYS_SEARCH_USER_PATHS=NO
