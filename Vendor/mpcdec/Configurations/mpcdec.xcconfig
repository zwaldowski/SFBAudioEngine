PRODUCT_NAME=mpcdec
CURRENT_PROJECT_VERSION=8.475
DYLIB_CURRENT_VERSION=8.48
FRAMEWORK_VERSION=8.475
DYLIB_COMPATIBILITY_VERSION=1

GCC_WARN_64_TO_32_BIT_CONVERSION=NO
GCC_WARN_ABOUT_MISSING_PROTOTYPES=NO
GCC_WARN_SHADOW=NO
GCC_WARN_UNUSED_VARIABLE=NO
GCC_WARN_UNUSED_FUNCTION=NO
GCC_WARN_SIGN_COMPARE=NO
CLANG_WARN_IMPLICIT_SIGN_CONVERSION=NO
CLANG_WARN_SUSPICIOUS_IMPLICIT_CONVERSION=NO
WARNING_CFLAGS=$(inherited) -Wno-tautological-compare

INFOPLIST_FILE[sdk=macosx*]=mpcdec-Info.plist
HEADER_SEARCH_PATHS=$(inherited) $(SRCROOT) $(SRCROOT)/mpcdec-src/include $(SRCROOT)/mpcdec-src/libwavformat
USER_HEADER_SEARCH_PATHS=$(inherited) $(SRCROOT)/musepack-src/include $(SRCROOT)

CLANG_ENABLE_MODULES=NO
