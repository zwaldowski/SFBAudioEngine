PRODUCT_NAME=speex
CURRENT_PROJECT_VERSION=1.2.0
DYLIB_CURRENT_VERSION=1.2.0
FRAMEWORK_VERSION=1.2.0
DYLIB_COMPATIBILITY_VERSION=1

GCC_WARN_64_TO_32_BIT_CONVERSION=NO
GCC_WARN_ABOUT_MISSING_PROTOTYPES=NO
GCC_WARN_SHADOW=NO
GCC_WARN_UNUSED_VARIABLE=NO
GCC_WARN_SIGN_COMPARE=NO
GCC_WARN_ABOUT_POINTER_SIGNEDNESS=NO
CLANG_WARN_IMPLICIT_SIGN_CONVERSION=NO
CLANG_WARN_SUSPICIOUS_IMPLICIT_CONVERSION=NO

GCC_PREPROCESSOR_DEFINITIONS=$(inherited) HAVE_CONFIG_H=1
INFOPLIST_FILE[sdk=macosx*]=$(SRCROOT)/Speex/Speex-Info.plist
HEADER_SEARCH_PATHS=$(inherited) $(SRCROOT)/Speex/speex-src/include $(SRCROOT)/Speex/speex-src/lib $(SRCROOT)/Speex
USER_HEADER_SEARCH_PATHS=$(inherited) $(SRCROOT)/Speex/speex-src/include $(SRCROOT)/Speex

CLANG_ENABLE_MODULES=NO
