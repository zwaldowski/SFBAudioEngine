PRODUCT_NAME=sndfile
CURRENT_PROJECT_VERSION=1.0.25
DYLIB_CURRENT_VERSION=1.0.25
FRAMEWORK_VERSION=1.0.25
DYLIB_COMPATIBILITY_VERSION=1

GCC_WARN_64_TO_32_BIT_CONVERSION=NO
GCC_WARN_UNUSED_VARIABLE=NO
CLANG_WARN_IMPLICIT_SIGN_CONVERSION=NO
CLANG_WARN_SUSPICIOUS_IMPLICIT_CONVERSION=NO
WARNING_CFLAGS="-Wno-format"

GCC_PREPROCESSOR_DEFINITIONS=$(inherited) HAVE_CONFIG_H
INFOPLIST_FILE[sdk=macosx*]=$(SRCROOT)/sndfile/sndfile-Info.plist
HEADER_SEARCH_PATHS=$(inherited) $(SRCROOT)/sndfile

CLANG_ENABLE_MODULES=NO
