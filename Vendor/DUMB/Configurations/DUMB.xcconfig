PRODUCT_NAME=dumb
CURRENT_PROJECT_VERSION=0.9.3
DYLIB_CURRENT_VERSION=0.9.3
DYLIB_COMPATIBILITY_VERSION=1

GCC_WARN_64_TO_32_BIT_CONVERSION=NO
GCC_WARN_SHADOW=NO
GCC_WARN_SIGN_COMPARE=NO
GCC_WARN_ABOUT_POINTER_SIGNEDNESS=NO
CLANG_WARN_IMPLICIT_SIGN_CONVERSION=NO
CLANG_WARN_SUSPICIOUS_IMPLICIT_CONVERSION=NO

INFOPLIST_FILE[sdk=macosx*]=DUMB-Info.plist
HEADER_SEARCH_PATHS=$(inherited) $(SRCROOT)/DUMB-src/include $(SRCROOT)
USER_HEADER_SEARCH_PATHS=$(inherited) $(SRCROOT)/DUMB-src/include/internal $(SRCROOT)
ALWAYS_SEARCH_USER_PATHS=NO
