PRODUCT_NAME=mpg123
CURRENT_PROJECT_VERSION=1.16.0
DYLIB_CURRENT_VERSION=1.16.0
FRAMEWORK_VERSION=1.16.0
DYLIB_COMPATIBILITY_VERSION=1

GCC_WARN_64_TO_32_BIT_CONVERSION=NO
GCC_WARN_ABOUT_MISSING_PROTOTYPES=NO
GCC_WARN_SHADOW=NO
GCC_WARN_UNUSED_FUNCTION=NO
GCC_WARN_SIGN_COMPARE=NO
CLANG_WARN_IMPLICIT_SIGN_CONVERSION=NO
CLANG_WARN_SUSPICIOUS_IMPLICIT_CONVERSION=NO
WARNING_CFLAGS=-Wno-format

INFOPLIST_FILE[sdk=macosx*]=$(SRCROOT)/mpg123-Info.plist
HEADER_SEARCH_PATHS=$(inherited) $(SRCROOT)/mpg123 $(SRCROOT) $(SRCROOT)/mpg123-src/src/libmpg123
USER_HEADER_SEARCH_PATHS=$(inherited) $(SRCROOT)/mpg123 $(SRCROOT)

CLANG_ENABLE_MODULES=NO
